  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/STANDARDLABEL">
    <at:authority-code>STANDARDLABEL</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>STANDARDLABEL</at:op-code>
    <at:start.use>2012-07-05</at:start.use>
    <atold:op-code>STANDARDLABEL</atold:op-code>
    <dc:identifier>STANDARDLABEL</dc:identifier>
    <skos:definition xml:lang="en">Preferred is a label type used in all authority tables to mark the mainly used label form.</skos:definition>
    <skos:example xml:lang="en">Example: ‘European Commission’ in Corporate body table, in English.</skos:example>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">preferred</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/ATTOLABEL">
    <at:authority-code>ATTOLABEL</at:authority-code>
    <at:deprecated>true</at:deprecated>
    <at:end.use>2019-09-17</at:end.use>
    <at:op-code>ATTOLABEL</at:op-code>
    <at:start.use>2018-02-28</at:start.use>
    <atold:op-code>ATTOLABEL</atold:op-code>
    <dc:identifier>ATTOLABEL</dc:identifier>
    <skos:definition xml:lang="en">ATTO was a label type in eProcurement-related authority tables to separate a label used in ATTO tables from preferred labels. ATTO is the internal translation manager application of the Publications Office of the European Union.</skos:definition>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">ATTO</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/GRAMMATICAL">
    <at:authority-code>GRAMMATICAL</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>GRAMMATICAL</at:op-code>
    <at:start.use>2021-06-16</at:start.use>
    <atold:op-code>GRAMMATICAL</atold:op-code>
    <dc:identifier>GRAMMATICAL</dc:identifier>
    <skos:definition xml:lang="en">Grammatical is a special label type which can be combined with other label types. It aims to mark multiple gender markings within a term or other grammatical relations. This type is not used yet.</skos:definition>
    <skos:example xml:lang="en">Example: the French version of ‘Assistant to the President’ in Role qualifier authority table can have four forms as both the assistant and the president can be a man or a woman:
Assistant du président (male/male)
Assistante du président (female/male)
Assistant de la présidente (male/female)
Assistante de la présidente (female/female)</skos:example>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">grammatical</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/ACRONYM">
    <at:authority-code>ACRONYM</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>ACRONYM</at:op-code>
    <at:start.use>2012-07-05</at:start.use>
    <atold:op-code>ACRONYM</atold:op-code>
    <dc:identifier>ACRONYM</dc:identifier>
    <skos:definition xml:lang="en">Acronym is a label type used in several authority tables to mark label form composed of the initial letters of the complet form of a name or expression.</skos:definition>
    <skos:example xml:lang="en">Example: ‘OP’ is the acronym for ‘Publications Office’ (preferred label) in Corporate body table, in English.</skos:example>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">acronym</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/CURRENCYSUBUNIT">
    <at:authority-code>CURRENCYSUBUNIT</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>CURRENCYSUBUNIT</at:op-code>
    <at:start.use>2012-07-05</at:start.use>
    <atold:op-code>CURRENCYSUBUNIT</atold:op-code>
    <dc:identifier>CURRENCYSUBUNIT</dc:identifier>
    <skos:definition xml:lang="en">Subunit is a label type used in Currency authority table to mark the fractions of the main currency unit.</skos:definition>
    <skos:example xml:lang="en">Example: ‘cent’ (singular form), ‘cents’ (plural form) is the subunit for ‘euro’, in English.</skos:example>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">subunit</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/FORMLABEL">
    <at:authority-code>FORMLABEL</at:authority-code>
    <at:deprecated>true</at:deprecated>
    <at:end.use>2019-09-17</at:end.use>
    <at:op-code>FORMLABEL</at:op-code>
    <at:start.use>2018-02-28</at:start.use>
    <atold:op-code>FORMLABEL</atold:op-code>
    <dc:identifier>FORMLABEL</dc:identifier>
    <skos:definition xml:lang="en">Form was a label type in eProcurement-related authority tables to separate a label used in forms from preferred labels.</skos:definition>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">form</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/SHORTLABEL">
    <at:authority-code>SHORTLABEL</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>SHORTLABEL</at:op-code>
    <at:start.use>2012-07-05</at:start.use>
    <atold:op-code>SHORTLABEL</atold:op-code>
    <dc:identifier>SHORTLABEL</dc:identifier>
    <skos:definition xml:lang="en">Short is a label type used in several authority tables to mark a label form different from the preferred label. The short label is generally shorter than the preferred one.</skos:definition>
    <skos:example xml:lang="en">Example: ‘Commission’ is the short label for ‘European Commission’ (preferred label) in Corporate body table, in English.</skos:example>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">short</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/LONGLABEL">
    <at:authority-code>LONGLABEL</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>LONGLABEL</at:op-code>
    <at:start.use>2012-07-05</at:start.use>
    <atold:op-code>LONGLABEL</atold:op-code>
    <dc:identifier>LONGLABEL</dc:identifier>
    <skos:definition xml:lang="en">Long is a label type used in several authority tables to mark a label form different from the preferred label. The long label is generally longer than the preferred one.</skos:definition>
    <skos:example xml:lang="en">Example: ‘Publications Office of the European Union’ is the long label for ‘Publications Office’ (preferred label) in Corporate body table, in English.</skos:example>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">long</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/SYMBOL">
    <at:authority-code>SYMBOL</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>SYMBOL</at:op-code>
    <at:start.use>2019-02-20</at:start.use>
    <atold:op-code>SYMBOL</atold:op-code>
    <dc:identifier>SYMBOL</dc:identifier>
    <skos:definition xml:lang="en">Symbol is a label type used in the Measurement unit authority table for displaying the symbols of different measurement units. These symbols and abbreviations can be different from SI (International System of Units) symbols in languages that use other than Latin alphabet.</skos:definition>
    <skos:example xml:lang="en">Example: ‘m’ is the symbol used for ‘metre’ in English.</skos:example>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">symbol</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/ABRIDGEDLABEL">
    <at:authority-code>ABRIDGEDLABEL</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>ABRIDGEDLABEL</at:op-code>
    <at:start.use>2014-11-26</at:start.use>
    <atold:op-code>ABRIDGEDLABEL</atold:op-code>
    <dc:identifier>ABRIDGEDLABEL</dc:identifier>
    <skos:definition xml:lang="en">Abridged is a label type used in Time period authority table to mark a label form for abbreviated label versions.</skos:definition>
    <skos:example xml:lang="en">Example: ‘Apr’ is the abridged label for ‘April’, in English.</skos:example>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">abridged</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/ALTERNATIVELABEL">
    <at:authority-code>ALTERNATIVELABEL</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>ALTERNATIVELABEL</at:op-code>
    <at:start.use>2012-07-05</at:start.use>
    <atold:op-code>ALTERNATIVELABEL</atold:op-code>
    <dc:identifier>ALTERNATIVELABEL</dc:identifier>
    <skos:definition xml:lang="en">Alternative is a label type used in several authority tables to mark a label form different from the preferred label, without specifying the type of difference. It is another, lesser used form to express the same concept.</skos:definition>
    <skos:example xml:lang="en">Example: ‘Sorani’ is an alternative label for ‘Central Kurdish’ in Language authority table, in English.</skos:example>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">alternative</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/DEMONYM">
    <at:authority-code>DEMONYM</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>DEMONYM</at:op-code>
    <at:start.use>2018-02-22</at:start.use>
    <atold:op-code>DEMONYM</atold:op-code>
    <dc:identifier>DEMONYM</dc:identifier>
    <skos:definition xml:lang="en">Demonym is a label type to mark the citizenship and/or inhabitant form related with a country. This label type is not used yet.</skos:definition>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">demonym</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://publications.europa.eu/resource/authority/label-type/OFFICIALLABEL">
    <at:authority-code>OFFICIALLABEL</at:authority-code>
    <at:deprecated>false</at:deprecated>
    <at:op-code>OFFICIALLABEL</at:op-code>
    <at:start.use>2012-07-05</at:start.use>
    <atold:op-code>OFFICIALLABEL</atold:op-code>
    <dc:identifier>OFFICIALLABEL</dc:identifier>
    <skos:definition xml:lang="en">Official is a label type used in Country authority table to mark the official state name of a country. In some cases this official label is the same as the preferred label which marks the geographical country name.</skos:definition>
    <skos:example xml:lang="en">Example: ‘Grand Duchy of Luxembourg’ is the official label for ‘Luxembourg’ (preferred label) in Country authority table, in English.</skos:example>
    <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
    <skos:prefLabel xml:lang="en">official</skos:prefLabel>
    <skos:topConceptOf rdf:resource="http://publications.europa.eu/resource/authority/label-type"/>
  </skos:Concept>
