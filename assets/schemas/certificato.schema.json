{
  "CertificatoDiCodiceFiscale": {
    "$id": "https://w3id.org/italia/age/schemas/CertificatoDiCodiceFiscale.v1.json",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "x-jsonld-type": "https://w3id.org/italia/onto/CPV/Person",
    "x-jsonld-context": "https://w3id.org/italia/age/sdgr/jsonld/CertificatoDiCodiceFiscale.v1.jsonld",
    "additionalProperties": false,
    "description": "Questo schema rappresenta un certificato di Codice Fiscale.",
    "required": [
      "given_name",
      "family_name",
      "date_of_birth",
      "tax_code"
    ],
    "properties": {
      "@context": {
        "const": {
          "@version": 1.1,
          "@vocab": "https://w3id.org/italia/onto/CPV/",
          "tax_code": "taxCode",
          "given_name": "givenName",
          "family_name": "familyName",
          "date_of_birth": "dateOfBirth"
        }
      },
      "tax_code": {
        "type": "string",
        "description": "Il codice fiscale. Associato a https://w3id.org/italia/onto/CPV/taxCode.",
        "example": "RSSMRA75L01H501A",
        "maxLength": 16,
        "minLength": 11
      },
      "hasPersonTitle": {
        "type": "string",
        "description": "Even if this field is not present in @context, it is\nresolved by \"@vocab\" and it exists, so it can be considered\na good match. TODO we can think about that anyway if that\nis the good way to do it."
      },
      "date_of_birth": {
        "format": "date",
        "type": "string"
      },
      "family_name": {
        "maxLength": 255,
        "type": "string"
      },
      "given_name": {
        "maxLength": 255,
        "type": "string"
      }
    }
  }
}